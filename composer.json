{
  "name": "mallto/easy-tool",
  "description": "tools",
  "type": "library",
  "keywords": [
    "laravel",
    "tools"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "never615",
      "email": "never615@gmail.com"
    }
  ],
  "require": {
    "doctrine/dbal": "^3.0",
    "endroid/qr-code": "^3.4",
    "intervention/image": "^2.4",
    "jeremeamia/superclosure": "^2.3",
    "overtrue/laravel-filesystem-qiniu": "^2.2",
    "overtrue/laravel-pinyin": "~3.0",
    "predis/predis": "^2.2",
    "rap2hpoutre/laravel-log-viewer": "^2.2.0",
    "qiniu/php-sdk": "^7.12.0",
    "laravel/ui": "^4.0",
    "guzzlehttp/guzzle": "^7.2.0",
    "codingyu/laravel-ueditor": "dev-11.x-dev",
    "mongodb/laravel-mongodb": "^4.1",
    "alibabacloud/aliyun-log-php-sdk": "0.6.*"
  },
  "require-dev": {
    "fakerphp/faker": "^1.9.1",
    "laravel/pint": "^1.0",
    "laravel/sail": "^1.18",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^8.1",
    "phpunit/phpunit": "^10.1",
    "spatie/laravel-ignition": "^2.0",
    "beyondcode/laravel-dump-server": "^2.0",
    "filp/whoops": "~2.0",
    "laravel/browser-kit-testing": "v7.2.0"
  },
  "autoload": {
    "psr-4": {
      "Mallto\\Tool\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "http-basic": {
      "codeup.aliyun.com": {
        "username": "mallto",
        "password": ""
      }
    },
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
