{
    "name": "mallto/easy-tool",
    "description": "tools",
    "type": "library",
    "keywords": [
        "laravel",
        "tools"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "never615",
            "email": "never615@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "endroid/qr-code": "^3.4",
        "intervention/image": "^2.4",
        "jeremeamia/superclosure": "^2.3",
        "laravel/framework": "^10.10",
        "laravel/helpers": "^1.1",
        "symfony/console": "^6.4",
        "laravel/horizon": "^5.0",
        "laravel/tinker": "^2.8",
        "doctrine/dbal": "^3.0",
        "overtrue/laravel-filesystem-qiniu": "^2.2",
        "overtrue/laravel-pinyin": "~3.0",
        "predis/predis": "^1.1",
        "rap2hpoutre/laravel-log-viewer": "^2.2.0",
        "laravel/passport": "^11.10.0",
        "lcobucci/jwt": "^5.1",
        "qiniu/php-sdk": "^7.12.0",
        "laravel/ui":"^4.0",
        "guzzlehttp/guzzle": "^7.2.0",
        "smartins/passport-multiauth": "dev-6.0-dev",
        "codingyu/laravel-ueditor": "dev-10.0.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Mallto\\Tool\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "http-basic": {
            "codeup.aliyun.com": {
                "username": "mallto",
                "password": ""
            }
        },
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
